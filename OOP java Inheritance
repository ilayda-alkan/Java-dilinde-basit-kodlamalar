//INHERİTANCE

public class Main {

	public static void main(String[] args) {

		Employee employee = new Employee("Mustafa" ,"Bilişim" ,3000);
		employee.showInfos();
		
		Manager manager = new Manager("Murat" ,"Satın Alma" ,4000,10);
		manager.showInfos();
		
		manager.raiseSalary(100);
		
		System.out.println(manager);
	}

}
